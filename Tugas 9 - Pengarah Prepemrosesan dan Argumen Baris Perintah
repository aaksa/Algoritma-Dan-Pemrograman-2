}

2.
#include <bits/stdc++.h>

using namespace std;

#define MIN(a,b) (((a) < (b)) ? a : b )   //mencari nilai minimum

int main()

{

 int f,k;

cin >> f >> k ;

cout << "Nilai terkecil adalah  : " <<< MIN(f,k) << endl;

return 0;

}

